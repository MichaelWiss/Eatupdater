<header id="content-header">
  <h1><%= pluralize(@users.size, 'User') %></h1>
</header>



<div class="row">
 <div class="small-4 large-4 columns">  
  <ul id="users">
    <% @users.each do |user| %>
      <li>
        <%= profile_image_for(user) %>
        <%= link_to user.name, user %>
       </li>
    <% end %>
   </ul>
  </div>
</div>